PORT=3001
NODE_ENV=development

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/fcg_storage

# API Keys (generate secure keys for production)
API_KEY_MAIN=your-main-api-key-here
API_KEY_SECONDARY=your-secondary-api-key-here

# Storage
UPLOAD_PATH=./uploads
MAX_FILE_SIZE=10485760
ALLOWED_MIME_TYPES=image/jpeg,image/png,image/gif,image/webp,application/pdf,application/msword,application/vnd.openxmlformats-officedocument.wordprocessingml.document

# Thumbnails
THUMBNAIL_WIDTH=300
THUMBNAIL_HEIGHT=300
THUMBNAIL_QUALITY=80

# CORS
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# Rate Limiting
THROTTLE_TTL=60
THROTTLE_LIMIT=10
